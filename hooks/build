#!/bin/bash

docker build -t $IMAGE_NAME \
--build-arg APPLICATION_ID=$APPLICATION_ID \
--build-arg BOT_TOKEN=$BOT_TOKEN \
--build-arg COMMANDS_DIR=$COMMANDS_DIR \
--build-arg COMPETITION_CHANNEL_ID=$COMPETITION_CHANNEL_ID \
--build-arg COMPETITION_CHANNEL_NAME=$COMPETITION_CHANNEL_NAME \
--build-arg COMPETITION_POST_ID=$COMPETITION_POST_ID \
--build-arg DISCORD_BASE_API=$DISCORD_BASE_API \
--build-arg GUILD_ID=$GUILD_ID \
--build-arg SECONDS_TO_DELETE_MESSAGE=$SECONDS_TO_DELETE_MESSAGE \
.